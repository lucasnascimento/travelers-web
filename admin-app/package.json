    "@tiptap/extension-text-style": "^2.3.0",
    "@tiptap/pm": "^2.3.0",
    "@tiptap/react": "^2.3.0",
    "@tiptap/starter-kit": "^2.3.0",
    "date-fns": "^3.3.1",
    "date-input-polyfill": "^2.14.0",
    "dompurify": "^3.0.8",
    "is-url": "^1.2.4",
