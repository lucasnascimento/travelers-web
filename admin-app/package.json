    "localforage": "^1.10.0",
    "match-sorter": "^6.3.3",
    "preline": "^2.0.3",
    "react": "^18.2.0",
    "react-currency-input-field": "^3.8.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-hook-form": "^7.49.3",
    "react-input-mask": "^3.0.0-alpha.2",
    "react-router-dom": "^6.21.3",
    "slate": "^0.103.0",
    "slate-history": "^0.100.0",
    "slate-react": "^0.102.0",
    "sort-by": "^1.2.0",
